{
  "name": "rz-navi",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "leaflet": "cp ./node_modules/leaflet/dist/leaflet.js ./www/ && cp ./node_modules/leaflet/dist/leaflet.css ./www/",
    "proto": "cp ./node_modules/protobufjs/dist/minimal/protobuf.min.js ./www/ && ./node_modules/protobufjs/bin/pbjs ../common/navi.proto --target static-module -w closure -o ./www/proto.js && cp ../common/navi.proto ./bin/",
    "paho": "cp ./node_modules/paho-client/src/paho-mqtt.js ./www/",
    "build": "npm run leaflet && npm run proto && npm run paho"
  },
